        pixel_number += 1;
        change_millis = current_millis + millis_per_mm;
      }
      if (pixel_number >= 0 && pixel_number < label_width && line_number >= 0 && line_number < label_height && pgm_read_byte(&(labels[label_number][line_number][pixel_number])) == 1 ) {
        servo.write(down_position);
      } else {
        servo.write(up_position);
      };
      if (pixel_number >= label_width) {
        pixel_number = label_width;
      };
    };
  };
  latch_triggered = false;
};
