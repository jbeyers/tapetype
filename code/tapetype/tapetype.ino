const int num_labels = 3;
const int label_height = 24;
const int label_width = 285;

// Create the servo objects
Servo servo;

// Pen up and down positions
const int up_position = 105;
const int down_position = 90;

// Stepper settings and calculations

// stepper inputs
const int st_in4 = 8;
const int st_in3 = 9;
const int st_in2 = 10;
const int st_in1 = 11;

// 2048 steps per revolution
// 8.6mm diameter of the sandpaper-covered pencil, about 27 mm per revolution.
// steps per mm computed at 75.8, measured length is about 76. Round to nearest integer.
const int steps_per_mm = 76;
// Stepper speed is 5mm/s
float stepper_speed = steps_per_mm * -5.0;
// 200 milliseconds per mm (each pixel is 1mm)
unsigned long millis_per_mm = 200;

// Stepping is blocking, and we want the start point more accurate than 1mm.
// 10 steps is about 1/8 of a mm, which is close enough.
const int steps_for_start = 10;

// servo pwm output
const int servo_pwm = 6;
// Timing switch to indicate the start of a rotation.
const int timing_in = 13;
// Start button
const int start_in = 12;
// Indicator if the stepper should be running.
bool go = false;

AccelStepper stepper(AccelStepper::FULL4WIRE,st_in1, st_in2, st_in3, st_in4);

// Timing sensor latch. True if in the printable area.
bool latch = false;

// We need a one-shot for when the latch is triggered.
// Advancing to the next line depends on this.
bool latch_follower = false;
bool latch_triggered = false;

// Which label are we on.
int label_number = -1;
// Which line are we on.
int line_number = -1;
// Which pixel are we on.
int pixel_number = -1;

// params for the debounce.
unsigned long last_low_millis;
unsigned long last_high_millis;
unsigned long current_millis;

// Countdown to the next millis when a servo change needs to happen
unsigned long change_millis;

void setup() { 
  // Set the stepper speed.
  stepper.setMaxSpeed(1000);
  stepper.setSpeed(stepper_speed );
  // Attach the servo
  servo.attach(servo_pwm);
  // Servo starts in up position.

  // Timing switch
  pinMode(timing_in, INPUT_PULLUP);
  // Start toggle.
  pinMode(start_in, INPUT_PULLUP);

};

void loop() { 

  // Get a value for millis to use throughout the rest of the loop.
  current_millis = millis();

  // Debounce the timing switch.
  if ( !digitalRead(timing_in)) {
      last_high_millis = current_millis;
      if (current_millis - last_low_millis > 50 && latch) {
          latch = false;
      }
  } else {
      last_low_millis = current_millis;
      if (current_millis - last_high_millis > 50 && !latch) {
          latch = true;
      }
  };
  
  // Create the one-shot
  if (latch && !latch_follower) {
    latch_triggered = true;
  } else {
    latch_triggered = false;
  };
  latch_follower = latch;


  if (!digitalRead(start_in) && !go) {
    go = true;
    label_number += 1;
    if (label_number >= num_labels) {
      label_number = 0;
    };
    line_number = -1;
    pixel_number = -1;
  };

  if (go) {
    // Must be called on every loop to let it run.
    stepper.runSpeed();
